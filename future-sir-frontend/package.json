{
  "$schema": "https://json.schemastore.org/package",
  "name": "future-sir-frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:application && npm run build:server",
    "build:application": "react-router build",
    "build:server": "vite build --config ./vite.server.config.ts",
    "dev": "nodemon --config nodemon.json",
    "preview": "cross-env NODE_ENV=production SESSION_COOKIE_SECURE=false node --env-file ./.env --import ./build/server/opentelemetry.js ./build/server/server.js",
    "start": "cross-env NODE_ENV=production node --import ./build/server/opentelemetry.js ./build/server/server.js",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ./",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.54.0",
    "@opentelemetry/sdk-metrics": "^1.29.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@opentelemetry/sdk-trace-node": "^1.29.0",
    "@react-router/express": "^7.0.2",
    "@react-router/node": "^7.0.2",
    "@react-router/serve": "^7.0.2",
    "clsx": "^2.1.1",
    "compression": "^1.7.5",
    "connect-redis": "^8.0.1",
    "cross-env": "^7.0.3",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "i18next": "^24.0.5",
    "i18next-http-backend": "^3.0.1",
    "ioredis": "^5.4.1",
    "isbot": "^5.1.17",
    "jose": "^5.9.6",
    "minimatch": "^10.0.1",
    "morgan": "^1.10.0",
    "oauth4webapi": "^3.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.3",
    "react-router": "^7.0.2",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^2.5.5",
    "winston": "^3.17.0",
    "winston-error-format": "^3.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@playwright/test": "^1.49.0",
    "@react-router/dev": "^7.0.2",
    "@testing-library/react": "^16.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/compression": "^1.7.5",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/express-session": "^1.18.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.13.0",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^5.4.11",
    "vite-plugin-static-copy": "^2.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "vitest-mock-extended": "^2.0.2"
  },
  "engines": {
    "node": ">=22"
  }
}
