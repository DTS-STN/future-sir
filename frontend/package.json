{
  "$schema": "https://json.schemastore.org/package",
  "name": "future-sir-frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:application && npm run build:server",
    "build:application": "react-router build",
    "build:server": "vite build --config ./vite.server.config.ts",
    "dev": "nodemon --config nodemon.json",
    "preview": "npm run build && cross-env NODE_ENV=production SESSION_COOKIE_SECURE=false node --env-file-if-exists=.env --import ./build/server/telemetry.js ./build/server/server.js",
    "start": "cross-env NODE_ENV=production node --import ./build/server/telemetry.js ./build/server/server.js",
    "checks": "node --experimental-strip-types ./checks-runner.ts",
    "test": "vitest",
    "test:e2e": "playwright test",
    "format:check": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --cache --check .",
    "format:write": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --write .",
    "lint:check": "eslint --cache ./",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@dts-stn/health-checks": "^2.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.56.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.57.2",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.57.2",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.57.2",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-router/express": "^7.2.0",
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "@valibot/i18n": "^1.0.0-rc.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "connect-redis": "^8.0.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "i18next": "^24.2.2",
    "i18next-fetch-backend": "^6.0.0",
    "ioredis": "^5.5.0",
    "isbot": "^5.1.23",
    "jose": "^6.0.8",
    "minimatch": "^10.0.1",
    "morgan": "^1.10.0",
    "oauth4webapi": "^3.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-idle-timer": "^5.7.2",
    "react-number-format": "^5.4.3",
    "react-phone-number-input": "^3.4.12",
    "react-router": "^7.2.0",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^3.0.2",
    "valibot": "^1.0.0-rc.2",
    "validator": "^13.12.0",
    "winston": "^3.17.0",
    "winston-error-format": "^3.0.1",
    "xstate": "^5.19.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@inquirer/checkbox": "^4.1.2",
    "@playwright/test": "^1.50.1",
    "@react-router/dev": "^7.2.0",
    "@tailwindcss/vite": "^4.0.9",
    "@testing-library/react": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/compression": "^1.7.5",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/express-session": "^1.18.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/source-map-support": "^0.5.10",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.7",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "nodemon": "^3.1.9",
    "ora": "^8.2.0",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.8",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3",
    "type-fest": "^4.35.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.1.1",
    "vite-plugin-static-copy": "^2.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6",
    "vitest-mock-extended": "^3.0.1"
  },
  "engines": {
    "node": ">=22"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node@^4.34.8"
    }
  }
}
