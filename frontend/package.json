{
  "$schema": "https://json.schemastore.org/package",
  "name": "future-sir-frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:application && npm run build:server",
    "build:application": "react-router build",
    "build:server": "vite build --config ./vite.server.config.ts",
    "dev": "nodemon --config nodemon.json",
    "preview": "npm run build && cross-env NODE_ENV=production SESSION_COOKIE_SECURE=false node --env-file-if-exists=.env --import ./build/server/telemetry.js ./build/server/server.js",
    "start": "cross-env NODE_ENV=production node --import ./build/server/telemetry.js ./build/server/server.js",
    "checks": "node --experimental-strip-types ./scripts/checks-runner.ts",
    "test": "vitest",
    "test:e2e": "playwright test",
    "format:check": "prettier --cache --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint --cache ./",
    "typecheck": "react-router typegen && tsc",
    "openapi-ts": "openapi-typescript ./other/specs/fsir-openapi.yml -o ./app/.server/shared/api/fsir-openapi-schema.ts && eslint --fix ./app/.server/shared/api/fsir-openapi-schema.ts && prettier --write ./app/.server/shared/api/fsir-openapi-schema.ts"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@dts-stn/health-checks": "^2.0.0",
    "@faker-js/faker": "^9.7.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.200.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.200.0",
    "@opentelemetry/resources": "^2.0.0",
    "@opentelemetry/sdk-metrics": "^2.0.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@opentelemetry/semantic-conventions": "^1.32.0",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-router/express": "^7.5.0",
    "@react-router/node": "^7.5.0",
    "@react-router/serve": "^7.5.0",
    "@tanstack/react-table": "^8.21.2",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "connect-redis": "^8.0.2",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "i18next": "^24.2.3",
    "i18next-fetch-backend": "^6.0.0",
    "ioredis": "^5.6.1",
    "isbot": "^5.1.26",
    "jose": "^6.0.10",
    "minimatch": "^10.0.1",
    "morgan": "^1.10.0",
    "oauth4webapi": "^3.5.0",
    "openapi-fetch": "^0.13.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.4.1",
    "react-idle-timer": "^5.7.2",
    "react-number-format": "^5.4.4",
    "react-phone-number-input": "^3.4.12",
    "react-router": "^7.5.0",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^3.2.0",
    "tiny-invariant": "^1.3.3",
    "valibot": "^1.0.0",
    "winston": "^3.17.0",
    "winston-error-format": "^3.0.2",
    "xstate": "^5.19.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@inquirer/checkbox": "^4.1.5",
    "@playwright/test": "^1.51.1",
    "@react-router/dev": "^7.5.0",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/compression": "^1.7.5",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/express-session": "^1.18.1",
    "@types/jmespath": "^0.15.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/coverage-v8": "^3.1.1",
    "commander": "^13.1.0",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jmespath": "^0.16.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.9",
    "openapi-typescript": "^7.6.1",
    "ora": "^8.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3",
    "type-fest": "^4.40.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.2.4",
    "vite-plugin-static-copy": "^2.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=22"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node@^4.34.8"
    }
  }
}
